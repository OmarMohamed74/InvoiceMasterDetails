  <DataSets>
    <DataSet Name="TotalInvoice">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select c.Id,
c.CustromerName as Customer,
COUNT(distinct D.InvoiceMasterId) as InvoicesCount,
SUM( D.SellPrice) as TotalPrice,
COUNT(P.Id) as ProductsCount
from Customers C 
inner join InvoiceMasters M on c.Id=M.CustomerId
inner join InvoiceDetails D on M.Id = D.InvoiceMasterId
inner join Products P on P.Id = D.ProductId
group by c.CustromerName ,c.Id

</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvoicesCount">
          <DataField>InvoicesCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalPrice">
          <DataField>TotalPrice</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ProductsCount">
          <DataField>ProductsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
